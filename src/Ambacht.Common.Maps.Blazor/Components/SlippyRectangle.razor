<path d="@Value?.GetSvgPath(View)" fill="@Fill" stroke="@Stroke" stroke-width="@StrokeWidth" style="pointer-events: none;" />

@code {


	[Parameter()]
	public LatLngBounds Value { get; set; }

	[Parameter()]
	public string Fill { get; set; } = "#dbdbdb";

	[Parameter()]
	public string Stroke { get; set; } = "#373737";

	[Parameter()]
	public float StrokeWidth { get; set; } = 1f;

	[CascadingParameter()]
	public SlippyMapView View { get; set; }


}
